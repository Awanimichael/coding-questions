    "def isPalindrome(string):\n",
    "    strlen = len(string)\n",
    "    if strlen <= 1:\n",
    "        return True\n",
    "    else:\n",
    "        if string[0] == string[strlen-1] and isPalindrome(string[0:strlen-1]):\n",
    "            return True"
   ]
